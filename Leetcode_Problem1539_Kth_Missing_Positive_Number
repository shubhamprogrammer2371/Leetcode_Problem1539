int findKthPositive(int* arr, int arrSize, int k){
    int temparr[2005] = {0};
    int counter = 0;
    for(int i = 0 ; i < arrSize ; i++)
        temparr[arr[i]] = 1;
    for( int i = 1 ; i < 2005 ; i++)
    {
        if(!(temparr[i]))
            counter++;
        if(counter == k)
            return i;
    }
    return 0;
}


